{
  "Kestrel": {
    "Endpoints": {
      "Localhost": {
        "Address": "127.0.0.1",
        "Port": "53354"
      },
      "LocalhostHttps": {
        "Address": "127.0.0.1",
        "Port": "44332",
        "Certificate": "HTTPS"
      }
    }
  },
  "Authentication": {
    "AzureAd": {
      "AzureAdInstance": "https://login.microsoftonline.com/",
      "ClientId": "[ client id ]",
      "Audience": "https://[ domain ]/DotNetCoreSpa.Api",
      "CallbackPath": "/signin-oidc",
      "Domain": "[ domain ]",
      "TenantId": "[ tenant id ]"
    }
  },
  "Logging": {
    "IncludeScopes": false,
    "LogLevel": {
      "Default": "Warning"
    }
  }
}
